<!DOCTYPE html>
<html>
  <head>
    <title>BnB</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['MAPS_API_KEY']}" %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
    <script type="text/javascript">
      window.logo ="<%= image_url("bikelogo.jpg")%>";
      window.track1 ="<%= image_url("track-pic.jpg")%>";
      window.splashvid ="<%= image_url("video1.gif")%>";
      window.offroad ="<%= image_url("dirtbike1.jpg")%>";
      window.sport ="<%= image_url("sportbike.jpg")%>";
      window.street ="<%= image_url("streetbike.jpg")%>";
      window.touring ="<%= image_url("touringbike.jpg")%>";
      window.dirt ="<%= image_url("dirtbike.jpg")%>";
   </script>
  <body>
    <%= yield %>
  </body>
</html>
